<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Do NOT back up wallet keys to cloud -->
        <exclude domain="sharedpref" path="meowcoin_secure_keys.xml" />
        <exclude domain="database" path="meowcoin_wallet.db" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="meowcoin_secure_keys.xml" />
    </device-transfer>
</data-extraction-rules>
